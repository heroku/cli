{
  "name": "heroku-cli",
  "description": "CLI to interact with Heroku",
  "version": "6.14.44",
  "author": "Jeff Dickey @jdxcode",
  "bin": {
    "heroku": "bin/run"
  },
  "bugs": "https://github.com/heroku/cli/issues",
  "cli-engine": {
    "bin": "heroku",
    "userPluginsEnabled": true,
    "dirname": "heroku",
    "defaultCommand": "dashboard",
    "npmRegistry": "https://cli-npm.heroku.com",
    "hooks": {
      "prerun": "./lib/hooks/prerun/analytics",
      "update": [
        "./lib/hooks/update/analytics",
        "./lib/hooks/update/tidy",
        "./lib/hooks/update/completions"
      ]
    },
    "s3": {
      "host": "cli-assets.heroku.com"
    },
    "corePlugins": [
      "heroku-apps",
      "heroku-certs",
      "heroku-ci",
      "heroku-cli-addons",
      "heroku-cli-oauth",
      "heroku-cli-status",
      "heroku-container-registry",
      "heroku-git",
      "heroku-local",
      "heroku-orgs",
      "heroku-pg",
      "heroku-pipelines",
      "heroku-ps-exec",
      "heroku-redis",
      "heroku-run",
      "heroku-spaces",
      "heroku-webhooks"
    ]
  },
  "dependencies": {
    "@cli-engine/command": "^12.0.3",
    "@cli-engine/config": "^5.0.10",
    "@cli-engine/engine": "https://github.com/heroku/cli-engine#ef4920e3",
    "@heroku-cli/command": "^7.0.10",
    "cli-flags": "^2.0.7",
    "cli-ux": "2.0.21",
    "debug": "3.1.0",
    "fs-extra": "5.0.0",
    "heroku-apps": "2.4.19",
    "heroku-certs": "1.1.44",
    "heroku-ci": "1.9.5",
    "heroku-cli-addons": "1.2.26",
    "heroku-cli-oauth": "2.0.15",
    "heroku-cli-status": "5.0.0",
    "heroku-container-registry": "4.4.1",
    "heroku-git": "2.5.23",
    "heroku-local": "5.1.21",
    "heroku-orgs": "1.7.1",
    "heroku-pg": "2.4.1",
    "heroku-pipelines": "2.5.0",
    "heroku-ps-exec": "2.2.1",
    "heroku-redis": "1.3.0",
    "heroku-run": "3.5.4",
    "heroku-spaces": "2.10.0",
    "heroku-webhooks": "1.1.0",
    "http-call": "4.0.6",
    "klaw": "2.1.1",
    "netrc-parser": "2.0.5",
    "semver": "5.4.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/ansi-styles": "^2.0.30",
    "@types/debug": "^0.0.30",
    "@types/fs-extra": "5.0.0",
    "@types/jest": "22.0.0",
    "@types/klaw": "2.1.1",
    "@types/nock": "9.1.0",
    "@types/node": "8.5.2",
    "@types/supports-color": "3.1.0",
    "del-cli": "1.1.0",
    "husky": "0.14.3",
    "jest": "22.0.4",
    "lint-staged": "6.0.0",
    "nock": "9.1.5",
    "prettier": "1.9.2",
    "ts-jest": "22.0.0",
    "typescript": "2.6.2"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=8.3.0"
  },
  "files": [
    "lib",
    "bin"
  ],
  "homepage": "https://cli.heroku.com",
  "keywords": [
    "heroku",
    "heroku-cli-plugin"
  ],
  "license": "ISC",
  "repository": "heroku/cli",
  "scripts": {
    "lint": "./scripts/test/lint",
    "posttest": "prettier -l 'src/**/*.ts'",
    "precommit": "./scripts/precommit && lint-staged",
    "prepare": "del-cli lib && tsc && del-cli 'lib/**/*.test.+(d.ts|js)'",
    "pretest": "tsc",
    "test": "./scripts/test/all"
  }
}

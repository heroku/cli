{
  "name": "heroku-cli",
  "description": "CLI to interact with Heroku",
  "version": "6.14.17",
  "author": "Jeff Dickey @jdxcode",
  "bin": {
    "heroku": "bin/run"
  },
  "bugs": "https://github.com/heroku/cli/issues",
  "cli-engine": {
    "bin": "heroku",
    "dirname": "heroku",
    "node": "8.4.0",
    "defaultCommand": "dashboard",
    "hooks": {
      "prerun": "./lib/hooks/prerun/analytics",
      "update": "./lib/hooks/update/analytics"
    },
    "s3": {
      "host": "cli-assets.heroku.com"
    },
    "plugins": [
      "heroku-apps",
      "heroku-certs",
      "heroku-ci",
      "heroku-cli-addons",
      "heroku-cli-oauth",
      "heroku-cli-status",
      "heroku-container-registry",
      "heroku-fork",
      "heroku-git",
      "heroku-local",
      "heroku-orgs",
      "heroku-pg",
      "heroku-pipelines",
      "heroku-ps-exec",
      "heroku-redis",
      "heroku-run",
      "heroku-spaces",
      "heroku-webhooks"
    ]
  },
  "dependencies": {
    "cli-engine": "4.3.1",
    "cli-engine-command": "7.0.6",
    "cli-engine-config": "2.0.1",
    "cli-engine-heroku": "2.0.0",
    "debug": "3.0.1",
    "fs-extra": "4.0.1",
    "heroku-apps": "2.4.12",
    "heroku-certs": "1.1.41",
    "heroku-ci": "1.9.5",
    "heroku-cli-addons": "1.2.25",
    "heroku-cli-oauth": "2.0.14",
    "heroku-cli-status": "4.0.5",
    "heroku-container-registry": "4.4.0",
    "heroku-fork": "4.1.25",
    "heroku-git": "2.5.21",
    "heroku-local": "5.1.21",
    "heroku-orgs": "1.6.6",
    "heroku-pg": "2.3.0",
    "heroku-pipelines": "2.2.0",
    "heroku-ps-exec": "2.2.0",
    "heroku-redis": "1.2.15",
    "heroku-run": "3.5.1",
    "heroku-spaces": "2.9.6",
    "heroku-webhooks": "1.0.7",
    "http-call": "3.0.2",
    "netrc-parser": "2.0.3",
    "semver": "5.4.1"
  },
  "devDependencies": {
    "@heroku/heroku-cli-dev-center-generator": "1.1.5",
    "babel-cli": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-preset-flow": "6.23.0",
    "eslint": "4.6.1",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-flowtype": "2.35.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "21.0.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "flow-bin": "0.54.0",
    "husky": "0.14.3",
    "jest": "21.0.1",
    "nock": "9.0.14"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=8.3.0"
  },
  "files": [
    "lib",
    "bin"
  ],
  "homepage": "https://cli.heroku.com",
  "keywords": [
    "heroku",
    "heroku-cli-plugin"
  ],
  "license": "ISC",
  "repository": "https://github.com/heroku/cli",
  "scripts": {
    "build": "babel src -d lib --ignore '*.test.js'",
    "clean": "rimraf lib tmp dist",
    "download": "./scripts/download-yarn",
    "lint": "./scripts/test/lint",
    "precommit": "./scripts/precommit",
    "prepare": "npm run build",
    "test": "./scripts/test/all"
  }
}

{
  "name": "heroku-cli",
  "description": "CLI to interact with Heroku",
  "version": "6.15.13",
  "author": "Jeff Dickey @jdxcode",
  "bin": {
    "heroku": "bin/run"
  },
  "bugs": "https://github.com/heroku/cli/issues",
  "cli-engine": {
    "bin": "heroku",
    "userPluginsEnabled": true,
    "dirname": "heroku",
    "defaultCommand": "dashboard",
    "npmRegistry": "https://cli-npm.heroku.com",
    "topics": {
      "2fa": {
        "description": "two-factor authentication",
        "hidden": true
      },
      "stack": {
        "description": "list available stacks",
        "hidden": true
      },
      "twofactor": {
        "description": "two-factor authentication",
        "hidden": true
      },
      "outbound-rules": {
        "description": "space outbound IP rules",
        "hidden": true
      }
    },
    "hooks": {
      "prerun": "./lib/hooks/prerun/analytics",
      "update": [
        "./lib/hooks/update/brew",
        "./lib/hooks/update/analytics",
        "./lib/hooks/update/tidy",
        "./lib/hooks/update/completions"
      ]
    },
    "s3": {
      "host": "cli-assets.heroku.com"
    },
    "corePlugins": [
      "heroku-apps",
      "heroku-certs",
      "heroku-ci",
      "heroku-cli-addons",
      "heroku-cli-oauth",
      "heroku-cli-status",
      "heroku-container-registry",
      "heroku-git",
      "heroku-local",
      "heroku-orgs",
      "heroku-pg",
      "heroku-pipelines",
      "heroku-ps-exec",
      "heroku-redis",
      "heroku-run",
      "heroku-spaces",
      "heroku-webhooks"
    ]
  },
  "dependencies": {
    "@cli-engine/command": "^12.1.0",
    "@cli-engine/config": "^5.1.0",
    "@cli-engine/engine": "^6.1.30",
    "@heroku-cli/command": "^7.0.13",
    "bcat": "^1.1.8",
    "cli-flags": "^2.0.7",
    "cli-ux": "2.0.21",
    "debug": "3.1.0",
    "execa": "^0.8.0",
    "fs-extra": "5.0.0",
    "heroku-apps": "2.4.19",
    "heroku-certs": "1.1.44",
    "heroku-ci": "1.9.5",
    "heroku-cli-addons": "1.2.26",
    "heroku-cli-oauth": "2.0.15",
    "heroku-cli-status": "5.0.0",
    "heroku-container-registry": "4.4.4",
    "heroku-git": "2.5.23",
    "heroku-local": "5.1.21",
    "heroku-orgs": "1.7.1",
    "heroku-pg": "2.4.1",
    "heroku-pipelines": "2.5.0",
    "heroku-ps-exec": "2.3.0",
    "heroku-redis": "1.3.0",
    "heroku-run": "3.5.4",
    "heroku-spaces": "2.10.0",
    "heroku-webhooks": "1.1.0",
    "http-call": "4.0.8",
    "netrc-parser": "3.0.2",
    "semver": "5.4.1",
    "tar-fs": "^1.16.0",
    "ts-lodash": "^4.0.11",
    "tslib": "^1.8.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@cli-engine/util": "^1.1.5",
    "@heroku-cli/tslint": "^1.1.4",
    "@types/ansi-styles": "^2.0.30",
    "@types/debug": "^0.0.30",
    "@types/execa": "^0.8.1",
    "@types/fs-extra": "5.0.0",
    "@types/jest": "22.0.1",
    "@types/lodash": "^4.14.92",
    "@types/nock": "9.1.1",
    "@types/node": "9.3.0",
    "@types/supports-color": "3.1.0",
    "del-cli": "1.1.0",
    "husky": "0.14.3",
    "jest": "22.0.4",
    "nock": "9.1.6",
    "prettier": "^1.9.2",
    "ts-jest": "22.0.1",
    "tslint": "^5.8.0",
    "typescript": "2.6.2"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=8.3.0"
  },
  "files": [
    "lib",
    "bin"
  ],
  "homepage": "https://cli.heroku.com",
  "keywords": [
    "heroku",
    "heroku-cli-plugin"
  ],
  "license": "ISC",
  "repository": "heroku/cli",
  "scripts": {
    "lint": "yarn run shellcheck",
    "posttest": "yarn run lint && cli-engine-util",
    "precommit": "yarn run lint && cli-engine-util",
    "prepare": "cli-engine-util",
    "pretest": "tsc",
    "shellcheck": "./scripts/shellcheck",
    "test": "jest"
  }
}

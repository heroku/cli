{
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@types/lolex": "^5.1.6",
    "@types/psl": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "cspell": "7.3.9",
    "eslint": "8.57.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.1.14",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.4.3",
    "eslint-plugin-n": "^16.6.2",
    "execa": "5.1.1",
    "husky": "8.0.3",
    "lerna": "^6.4.1",
    "mkdirp": "^0.5.2",
    "oclif": "4.17.46",
    "qqjs": "0.3.11",
    "standard": "17.1.0",
    "tmp": "^0.2.1",
    "ts-node": "^10.9.2"
  },
  "overrides": {
    "ansi-regex": "4.1.1",
    "@heroku/http-call": "^5.5.0"
  },
  "standard": {
    "env": "mocha",
    "ignore": [
      "**/node_modules/**",
      "packages/*/lib/**"
    ]
  },
  "engines": {
    "node": ">=20"
  },
  "private": true,
  "scripts": {
    "build": "cd packages/cli && npm run prepack",
    "test": "lerna run test --concurrency 4",
    "lint": "cd packages/cli && npm run lint",
    "cspell": "cspell",
    "version": "cp packages/cli/CHANGELOG.md CHANGELOG.md && git add CHANGELOG.md",
    "cleanNodeModules": "rm -rf ./packages/*/node_modules && rm -rf ./node_modules",
    "postinstall": "husky install"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}

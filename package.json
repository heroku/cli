{
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@types/lolex": "^5.1.6",
    "@types/psl": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "cspell": "9.2.0",
    "eslint": "^9.33.0",
    "eslint-config-oclif": "^6.0.95",
    "eslint-config-oclif-typescript": "^3.1.14",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^11.1.0",
    "execa": "5.1.1",
    "husky": "9.1.7",
    "lerna": "^8.2.3",
    "mkdirp": "^3.0.1",
    "oclif": "4.22.12",
    "qqjs": "0.3.11",
    "standard": "17.1.2",
    "tmp": "^0.2.1"
  },
  "resolutions": {
    "ansi-regex": "4.1.1"
  },
  "standard": {
    "env": "mocha",
    "ignore": [
      "**/node_modules/**",
      "packages/*/lib/**"
    ]
  },
  "engines": {
    "node": ">=14"
  },
  "private": true,
  "scripts": {
    "build": "cd packages/cli && yarn prepack",
    "test": "lerna run test --concurrency 4",
    "lint": "cd packages/cli && yarn lint",
    "version": "cp packages/cli/CHANGELOG.md CHANGELOG.md && git add CHANGELOG.md",
    "cleanNodeModules": "rm -rf ./packages/*/node_modules && rm -rf ./node_modules && rm -rf ./.yarn/cache",
    "postinstall": "husky install"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "packageManager": "yarn@3.6.1"
}

{
  "name": "heroku-cli",
  "description": "CLI to interact with Heroku",
  "version": "6.4.9",
  "author": "Jeff Dickey @dickeyxxx",
  "bin": {
    "heroku": "bin/run"
  },
  "bugs": "https://github.com/heroku/cli/issues",
  "cli-engine": {
    "bin": "heroku",
    "node": "7.8.0",
    "defaultCommand": "dashboard",
    "s3": {
      "bucket": "heroku-cli-assets",
      "host": "cli-assets.heroku.com"
    },
    "plugins": [
      "heroku-apps",
      "heroku-certs",
      "heroku-cli-addons",
      "heroku-cli-oauth",
      "heroku-fork",
      "heroku-git",
      "heroku-local",
      "heroku-orgs",
      "heroku-pg",
      "heroku-pipelines",
      "heroku-redis",
      "heroku-run",
      "heroku-spaces",
      "heroku-status"
    ]
  },
  "dependencies": {
    "cli-engine": "3.0.0",
    "heroku-apps": "2.2.19",
    "heroku-certs": "1.1.38",
    "heroku-cli-addons": "1.2.16",
    "heroku-cli-oauth": "2.0.12",
    "heroku-fork": "4.1.21",
    "heroku-git": "2.5.16",
    "heroku-local": "5.1.13",
    "heroku-orgs": "1.5.12",
    "heroku-pg": "2.0.25",
    "heroku-pipelines": "1.4.4",
    "heroku-redis": "1.2.13",
    "heroku-run": "3.4.10",
    "heroku-spaces": "2.8.6",
    "heroku-status": "3.0.10"
  },
  "files": [
    "bin"
  ],
  "homepage": "https://cli.heroku.com",
  "keywords": [
    "heroku",
    "heroku-cli-plugin"
  ],
  "license": "ISC",
  "repository": "https://github.com/heroku/cli",
  "scripts": {
    "build": "cli-build",
    "release": "cli-release"
  }
}

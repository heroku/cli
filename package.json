{
  "dependencies": {
    "@heroku-cli/color": "1.1.14",
    "@heroku-cli/command": "9.0.1",
    "@heroku-cli/schema": "1.0.25",
    "@oclif/core": "1.22.0"
  },
  "devDependencies": {
    "@oclif/tslint": "^3.1.1",
    "eslint": "5.13.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "lerna": "^3.22.1",
    "promise-request-retry": "^1.0.2",
    "standard": "12.0.1",
    "tmp": "^0.2.1"
  },
  "standard": {
    "env": "mocha",
    "ignore": [
      "**/node_modules/**",
      "packages/*/lib/**"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "private": true,
  "scripts": {
    "test": "lerna run test --concurrency 4",
    "version": "cp packages/cli/CHANGELOG.md CHANGELOG.md && git add CHANGELOG.md",
    "cleanNodeModules": "rm -rf ./packages/*/node_modules & rm -rf ./node_modules"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/heroku-cli-util"
    ]
  }
}

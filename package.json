{
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@types/psl": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "cspell": "9.6.0",
    "eslint": "8.57.0",
    "eslint-config-oclif": "^5.2.2",
    "eslint-config-oclif-typescript": "^3.1.14",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.4.3",
    "eslint-plugin-n": "^17.23.2",
    "execa": "5.1.1",
    "husky": "8.0.3",
    "lerna": "^6.4.1",
    "commit-and-tag-version": "^12.5.0",
    "oclif": "^4.22.27",
    "qqjs": "0.3.11",
    "tmp": "^0.2.5",
    "ts-node": "^10.9.2"
  },
  "overrides": {
    "ansi-regex": "4.1.1"
  },
  "engines": {
    "node": ">=20"
  },
  "private": true,
  "scripts": {
    "build": "cd packages/cli && npm run prepack",
    "test": "npm run test --workspace=packages/cli",
    "lint": "cd packages/cli && npm run lint",
    "lint:fix": "cd packages/cli && npm run lint:fix",
    "cspell:ci": "cspell --no-progress --gitignore --no-must-find-files --file-list stdin",
    "cspell:commit": "cspell --no-progress --gitignore --no-must-find-files --no-show-suggestions --config ./cspell.json --quiet",
    "pack:deb": "oclif pack:deb --root=\"./packages/cli\"",
    "pack:tarballs": "oclif pack:tarballs --root=\"./packages/cli\" --xz --parallel",
    "pack:win": "oclif pack:win --root=\"./packages/cli\" --defender-exclusion hidden",
    "promote:deb": "oclif promote --root=\"./packages/cli\" --deb --xz --indexes",
    "promote:win": "oclif promote --root=\"./packages/cli\" --win --indexes",
    "upload:deb": "oclif upload:deb --root=\"./packages/cli\"",
    "upload:tarballs": "oclif upload:tarballs --root=\"./packages/cli\" --xz",
    "upload:win": "oclif upload:win --root=\"./packages/cli\"",
    "cleanNodeModules": "rm -rf ./packages/*/node_modules && rm -rf ./node_modules",
    "postinstall": "husky install"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}

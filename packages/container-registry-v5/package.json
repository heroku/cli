{
  "name": "@heroku-cli/plugin-container-registry-v5",
  "description": "Use containers to build and deploy Heroku apps",
  "version": "8.6.0",
  "author": "Heroku",
  "bugs": "https://github.com/heroku/cli/issues",
  "cli-engine": {
    "topics": {
      "container": {
        "description": "Use containers to build and deploy Heroku apps"
      }
    },
    "repositoryPrefix": "<%- repo %>/blob/v<%- version %>/packages/container-registry-v5/<%- commandPath %>"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "glob": "^7.1.3",
    "heroku-cli-util": "^8.0.11",
    "heroku-container-registry": "^4.99.0",
    "http-call": "^5.2.3",
    "inquirer": "^6.2.2"
  },
  "devDependencies": {
    "@oclif/plugin-legacy": "^1.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.2",
    "depcheck": "^1.4.3",
    "lolex": "^3.1.0",
    "mocha": "^5.0.4",
    "mockdate": "^2.0.2",
    "nock": "^10.0.6",
    "nyc": "^15.1.0",
    "oclif": "3.11.3",
    "sinon": "^5.0.7",
    "std-mocks": "^1.0.1"
  },
  "files": [
    "oclif.manifest.json",
    "index.js",
    "commands",
    "lib"
  ],
  "homepage": "https://github.com/heroku/cli/tree/main/packages/container-registry-v5",
  "keywords": [
    "cli",
    "container",
    "heroku",
    "heroku-plugin"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": "heroku/cli",
  "scripts": {
    "depcheck": "depcheck || true",
    "lint": "eslint . --ext .js --config ../../.eslintrc --ignore-path ../../.eslintignore",
    "postpublish": "rm oclif.manifest.json",
    "prepack": "oclif manifest",
    "test": "cross-env TZ=utc nyc mocha --forbid-only \"test/**/*.unit.test.js\" && yarn lint",
    "version": "oclif readme && git add README.md"
  },
  "topic": "container"
}

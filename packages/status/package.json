{
  "name": "@heroku-cli/plugin-status",
  "description": "CLI Plugin for Heroku Status",
  "version": "7.39.2",
  "author": "Jeff Dickey @jdxcode",
  "bugs": "https://github.com/heroku/cli/issues",
  "cli-engine": {
    "commands": "./lib/commands",
    "topics": {
      "status": {
        "description": "status of the Heroku platform"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "repositoryPrefix": "<%- repo %>/blob/v<%- version %>/packages/status/<%- commandPath %>"
  },
  "dependencies": {
    "@heroku-cli/color": "^1.1.14",
    "@heroku-cli/command": "^8.3.0",
    "@oclif/command": "^1.5.11",
    "@oclif/config": "^1.12.10",
    "@oclif/errors": "^1.2.2",
    "cli-ux": "^4.9.3",
    "date-fns": "^1.29.0",
    "http-call": "^5.2.3"
  },
  "devDependencies": {
    "@fancy-test/nock": "^0.1.1",
    "@heroku-cli/tslint": "1.1.4",
    "@oclif/dev-cli": "^1.21.3",
    "@oclif/plugin-help": "^2.1.6",
    "@oclif/plugin-legacy": "^1.1.4",
    "@oclif/test": "^1.2.4",
    "@types/ansi-styles": "^3.2.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/nock": "^9.3.1",
    "@types/node": "^10.12.24",
    "@types/supports-color": "^5.3.0",
    "chai": "^4.2.0",
    "eslint": "^6.7.2",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^9.0.0",
    "mocha": "^5.1.1",
    "nock": "^10.0.6",
    "ts-node": "^8.0.2",
    "typescript": "3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "oclif.manifest.json",
    "lib"
  ],
  "homepage": "https://github.com/heroku/cli/tree/master/packages/status",
  "keywords": [
    "heroku-plugin"
  ],
  "license": "ISC",
  "repository": "heroku/cli",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpublish": "rm -f oclif.manifest.json",
    "prepack": "yarn run build && oclif-dev manifest",
    "preversion": "yarn run postpublish",
    "pretest": "tsc -p test --noEmit",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "yarn lint",
    "version": "oclif-dev readme && git add README.md"
  }
}

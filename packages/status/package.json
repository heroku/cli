{
  "name": "@heroku-cli/plugin-status",
  "description": "CLI Plugin for Heroku Status",
  "version": "7.16.3",
  "author": "Jeff Dickey @jdxcode",
  "bugs": "https://github.com/heroku/cli/issues",
  "cli-engine": {
    "commands": "./lib/commands",
    "topics": {
      "status": {
        "description": "status of the Heroku platform"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "repositoryPrefix": "<%- repo %>/blob/v<%- version %>/packages/status/<%- commandPath %>"
  },
  "dependencies": {
    "@heroku-cli/color": "^1.1.10",
    "@heroku-cli/command": "^8.1.29",
    "@oclif/command": "^1.5.2",
    "@oclif/config": "^1.8.2",
    "@oclif/errors": "^1.2.1",
    "cli-ux": "^4.8.2",
    "date-fns": "^1.29.0",
    "http-call": "^5.2.2"
  },
  "devDependencies": {
    "@fancy-test/nock": "^0.1.1",
    "@heroku-cli/tslint": "1.1.4",
    "@oclif/dev-cli": "^1.19.0",
    "@oclif/plugin-help": "^2.1.2",
    "@oclif/plugin-legacy": "^1.1.1",
    "@oclif/test": "^1.2.1",
    "@oclif/tslint": "^3.0.1",
    "@types/ansi-styles": "^3.2.0",
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.11.4",
    "@types/supports-color": "^5.3.0",
    "chai": "^4.2.0",
    "globby": "^8.0.1",
    "mocha": "^5.1.1",
    "nock": "^9.2.5",
    "ts-node": "^7.0.1",
    "tslint": "5.11.0",
    "typescript": "^3.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "oclif.manifest.json",
    "lib"
  ],
  "homepage": "https://github.com/heroku/cli/tree/master/packages/status",
  "keywords": [
    "heroku-plugin"
  ],
  "license": "ISC",
  "repository": "heroku/cli",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish",
    "postpublish": "rm -f oclif.manifest.json",
    "posttest": "yarn run lint",
    "prepack": "yarn run build && oclif-dev manifest",
    "preversion": "yarn run postpublish",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}

{
  "name": "@heroku-cli/plugin-status",
  "description": "CLI Plugin for Heroku Status",
  "version": "7.2.0",
  "author": "Jeff Dickey @jdxcode",
  "bugs": {
    "url": "https://github.com/heroku/heroku-cli-status/issues"
  },
  "cli-engine": {
    "commands": "./lib/commands",
    "topics": {
      "status": {
        "description": "status of the Heroku platform"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@heroku-cli/color": "^1.1.3",
    "@heroku-cli/command": "^8.0.6",
    "@oclif/command": "^1.4.18",
    "@oclif/config": "^1.6.16",
    "@oclif/errors": "^1.0.7",
    "cli-ux": "^4.7.1",
    "date-fns": "^1.29.0",
    "http-call": "^5.1.1"
  },
  "devDependencies": {
    "@fancy-test/nock": "^0.1.1",
    "@heroku-cli/tslint": "1.1.4",
    "@oclif/dev-cli": "^1.13.13",
    "@oclif/plugin-help": "^2.0.5",
    "@oclif/plugin-legacy": "^1.0.13",
    "@oclif/test": "^1.0.6",
    "@oclif/tslint": "^1.1.0",
    "@types/ansi-styles": "^3.2.0",
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/nock": "^9.1.3",
    "@types/node": "^10.0.2",
    "@types/supports-color": "^5.3.0",
    "chai": "^4.1.2",
    "globby": "^8.0.1",
    "mocha": "^5.1.1",
    "nock": "^9.2.5",
    "ts-node": "^6.0.2",
    "tslint": "5.10.0",
    "typescript": "^2.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "lib"
  ],
  "homepage": "https://github.com/heroku/heroku-cli-status",
  "keywords": [
    "heroku-plugin"
  ],
  "license": "ISC",
  "repository": "heroku/heroku-cli-status",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish",
    "postpublish": "rm -f .oclif.manifest.json",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build && oclif-dev manifest",
    "preversion": "yarn run postpublish",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}

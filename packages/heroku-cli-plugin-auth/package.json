{
  "name": "@heroku-cli/plugin-auth",
  "description": "auth core plugin for Heroku CLI",
  "version": "0.3.1",
  "author": "Jeff Dickey @jdxcode",
  "bugs": "https://github.com/heroku/heroku-cli-plugin-auth/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.3",
    "@heroku-cli/command": "^8.1.14",
    "@oclif/command": "^1.4.21",
    "@oclif/config": "^1.6.17",
    "cli-ux": "^4.2.0"
  },
  "devDependencies": {
    "@fancy-test/nock": "^0.1.1",
    "@oclif/dev-cli": "1.13.19",
    "@oclif/plugin-help": "1.2.10",
    "@oclif/test": "1.0.6",
    "@oclif/tslint": "1.1.1",
    "@types/chai": "4.1.3",
    "@types/mocha": "5.2.0",
    "@types/nock": "^9.1.3",
    "@types/node": "10.0.6",
    "@types/supports-color": "^5.3.0",
    "chai": "4",
    "globby": "8",
    "mocha": "5",
    "nock": "^9.2.5",
    "nyc": "11.7.3",
    "ts-node": "6.0.3",
    "tslib": "1",
    "tslint": "5.10.0",
    "typescript": "2.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib"
  ],
  "homepage": "https://github.com/heroku/heroku-cli-plugin-auth",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/heroku-cli-plugin-auth",
  "scripts": {
    "postpack": "rm -f .oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}

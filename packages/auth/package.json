{
  "name": "@heroku-cli/plugin-auth",
  "description": "auth core plugin for Heroku CLI",
  "version": "8.0.6",
  "author": "Jeff Dickey @jdxcode",
  "bugs": "https://github.com/heroku/cli/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.14",
    "@heroku-cli/command": "^9.0.2",
    "@oclif/core": "^1.26.2",
    "date-fns": "^2.0.0-alpha.8"
  },
  "devDependencies": {
    "@heroku-cli/schema": "^1.0.25",
    "@oclif/test": "^2.2.20",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/nock": "^9.3.1",
    "@types/supports-color": "^5.3.0",
    "chai": "^4.2.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nyc": "^15.1.0",
    "oclif": "3.8.1",
    "ts-node": "^10.9.1",
    "tslib": "^1.9.3",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "oclif.manifest.json",
    "/lib"
  ],
  "homepage": "https://github.com/heroku/cli/tree/main/packages/auth",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "commands": "./lib/commands",
    "bin": "heroku",
    "repositoryPrefix": "<%- repo %>/blob/v<%- version %>/packages/auth/<%- commandPath %>"
  },
  "repository": "heroku/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "pretest": "tsc -p test --noEmit",
    "posttest": "yarn lint",
    "lint": "eslint . --ext .ts --config ../../.eslintrc --ignore-path ../../.eslintignore-lib",
    "prepack": "rm -rf lib && tsc && oclif manifest",
    "prepare": "rm -rf lib && tsc && oclif manifest",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  }
}

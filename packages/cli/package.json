    "@types/node-fetch": "^2.6.11",
    "@types/phoenix": "^1.4.0",
    "@types/proxyquire": "^1.3.28",
    "@types/psl": "^1.1.3",
    "@types/redis-parser": "^3.0.3",
    "@types/shell-escape": "^0.2.0",
    "@types/shell-quote": "^1.7.5",
    "@types/sinon": "^17.0.3",
    "@types/ssh2": "^1.15.0",
    "@types/std-mocks": "^1.0.4",
    "@types/strftime": "^0.9.8",
    "@types/supports-color": "^8.1.3",
    "@types/tmp": "^0.2.6",
    "@types/urijs": "^1.19.4",

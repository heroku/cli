    "@types/ansi-styles": "^3.2.1",
    "@types/bytes": "^3.1.4",
    "@types/chai": "^4.3.14",
    "@types/chai-as-promised": "^7.1.8",
    "@types/debug": "^4.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^7.1.1",

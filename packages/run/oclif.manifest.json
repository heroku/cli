        "ps": {
          "name": "ps",
          "type": "option",
          "char": "p",
          "description": "hidden alias for dyno",
          "hidden": true,
          "multiple": false
        },
        "dyno": {
          "name": "dyno",
          "type": "option",
          "char": "d",
          "description": "only show output from this dyno type (such as \"web\" or \"worker\")",
          "multiple": false

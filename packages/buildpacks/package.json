{
  "name": "@heroku-cli/plugin-buildpacks",
  "version": "8.1.7",
  "author": "Terence Lee @hone",
  "bugs": "https://github.com/heroku/cli/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.14",
    "@heroku-cli/command": "^9.0.2",
    "@heroku/buildpack-registry": "^1.0.1",
    "@oclif/core": "^1.21.0",
    "heroku-cli-util": "^8.0.11",
    "http-call": "^5.2.3",
    "lodash": "^4.17.11",
    "true-myth": "2.2.3",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.25",
    "@types/ansi-styles": "^3.2.1",
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/nock": "^9.3.1",
    "@types/node-fetch": "^2.1.6",
    "@types/supports-color": "^5.3.0",
    "chai": "^4.2.0",
    "globby": "^10.0.2",
    "mocha": "^5",
    "nock": "^10.0.6",
    "nyc": "^15.1.0",
    "oclif": "3.8.1",
    "tmp": "^0.0.33",
    "ts-node": "^10.9.1",
    "tslib": "^1",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/heroku/cli",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "commands": "./lib/commands",
    "bin": "heroku",
    "repositoryPrefix": "<%- repo %>/blob/v<%- version %>/packages/buildpacks/<%- commandPath %>"
  },
  "repository": "heroku/cli",
  "scripts": {
    "lint": "eslint . --ext .ts --config ../../.eslintrc --ignore-path ../../.eslintignore-lib",
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "prepack": "rm -rf lib && tsc && oclif manifest",
    "prepare": "rm -rf lib && tsc",
    "pretest": "tsc -p test --noEmit",
    "test": "nyc mocha --forbid-only \"test/**/*.unit.test.ts\"",
    "posttest": "yarn lint",
    "version": "oclif readme && git add README.md"
  }
}

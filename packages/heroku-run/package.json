{
  "name": "@heroku-cli/plugin-run",
  "description": "Heroku CLI plugin to run one-off dyno processes.",
  "version": "3.8.4",
  "author": "Jeff Dickey (@dickeyxxx)",
  "bugs": {
    "url": "https://github.com/heroku/heroku-run/issues"
  },
  "cli-engine": {
    "topics": {
      "run": {
        "description": "run a one-off process inside a Heroku dyno"
      },
      "logs": {
        "description": "display recent log output"
      }
    }
  },
  "dependencies": {
    "@heroku-cli/color": "^1.1.3",
    "@heroku-cli/command": "^8.1.3",
    "@heroku-cli/notifications": "^1.2.0",
    "co": "4.6.0",
    "eventsource": "^1.0.5",
    "fs-extra": "^6.0.0",
    "heroku-cli-util": "^8.0.8",
    "shellwords": "^0.1.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.19",
    "@oclif/plugin-legacy": "^1.0.15",
    "fixture-stdout": "0.2.1",
    "mocha": "^5.1.1",
    "mocha-junit-reporter": "1.17.0",
    "netrc": "0.1.4",
    "nock": "^9.2.5",
    "sinon": "^5.0.7",
    "standard": "^11.0.1",
    "unexpected": "^10.37.7"
  },
  "standard": {
    "env": "mocha"
  },
  "files": [
    ".oclif.manifest.json",
    "lib",
    "index.js",
    "commands"
  ],
  "homepage": "https://github.com/heroku/heroku-run",
  "keywords": [
    "heroku-plugin"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/heroku-run.git"
  },
  "scripts": {
    "postpublish": "rm .oclif.manifest.json",
    "prepublishOnly": "oclif-dev manifest",
    "test": "mocha",
    "posttest": "standard",
    "version": "oclif-dev readme && git add README.md"
  }
}

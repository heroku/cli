{
  "name": "@heroku-cli/plugin-apps",
  "version": "8.0.0",
  "author": "Brett @brettgoulder",
  "bugs": "https://github.com/heroku/heroku-cli-plugin-apps/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.14",
    "@heroku-cli/command": "^9.0.1",
    "@heroku-cli/schema": "^1.0.25",
    "@oclif/core": "^1.22.0",
    "inquirer": "^7.0.1",
    "shell-escape": "^0.2.0",
    "tslib": "^1",
    "urijs": "^1.19.11"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.20",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/shell-escape": "^0.2.0",
    "@types/supports-color": "^5.3.0",
    "@types/urijs": "^1.19.4",
    "chai": "^4",
    "eslint": "^6.7.1",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10",
    "mocha": "^5",
    "nock": "^13.0.1",
    "nyc": "^15.1.0",
    "oclif": "3.4.3",
    "ts-node": "^10",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/heroku/heroku-cli-plugin-apps",
  "keywords": [
    "heroku-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "bin": "heroku",
    "commands": "./lib/commands"
  },
  "repository": "heroku/heroku-cli-plugin-apps",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "pretest": "tsc -p test --noEmit",
    "posttest": "yarn lint",
    "prepack": "rm -rf lib && tsc -b && oclif manifest",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  }
}

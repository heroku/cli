{
  "name": "@heroku-cli/plugin-ci",
  "description": "Heroku CLI plugin for Heroku CI",
  "version": "8.1.4",
  "author": "Raúl Barroso @raulb",
  "bugs": "https://github.com/heroku/cli/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.14",
    "@heroku-cli/command": "^9.0.2",
    "@oclif/core": "^1.26.2",
    "ansi-escapes": "3.2.0",
    "async-file": "^2.0.2",
    "debug": "^4.1.1",
    "fs-extra": "^7.0.1",
    "github-url-to-object": "^4.0.4",
    "got": "^9.6.0",
    "inquirer": "^6.2.2",
    "phoenix": "^1.6.14",
    "tmp": "^0.0.33",
    "tslib": "^1.9.3",
    "uuid": "^8.3.0",
    "validator": "^13.7.0",
    "ws": "^6.2.2"
  },
  "devDependencies": {
    "@fancy-test/nock": "^0.1.1",
    "@heroku-cli/schema": "^1.0.25",
    "@oclif/test": "^2.2.20",
    "@types/chai": "^4.1.7",
    "@types/inquirer": "7.3.0",
    "@types/mocha": "^5.2.6",
    "@types/nock": "^9.3.1",
    "@types/phoenix": "^1.4.0",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^10.9.0",
    "@types/ws": "^6.0.1",
    "chai": "^4.2.0",
    "globby": "^10.0.2",
    "mocha": "^5",
    "nock": "^10.0.6",
    "nyc": "^15.1.0",
    "oclif": "3.8.1",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/heroku/cli",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "bin": "heroku",
    "commands": "./lib/commands",
    "repositoryPrefix": "<%- repo %>/blob/v<%- version %>/packages/ci/<%- commandPath %>"
  },
  "repository": "heroku/cli",
  "scripts": {
    "lint": "eslint . --ext .ts --config ../../.eslintrc --ignore-path ../../.eslintignore-lib",
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "prepack": "rm -rf lib && tsc && oclif manifest",
    "prepare": "rm -rf lib && tsc",
    "pretest": "tsc -p test --noEmit",
    "test": "nyc mocha --forbid-only \"test/**/*.unit.test.ts\"",
    "posttest": "yarn lint",
    "version": "oclif readme && git add README.md"
  }
}
